{
  "name": "canigraduate",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "precommit": "precise-commits"
  },
  "dependencies": {
    "@firebase/app-types": "^0.3.2",
    "@tweenjs/tween.js": "^17.2.0",
    "algoliasearch": "^3.27.1",
    "axios": "^0.18.0",
    "base64-js": "^1.3.0",
    "chartist": "^0.11.0",
    "fastbitset": "^0.2.7",
    "firebase": "^5.0.4",
    "logrocket": "^0.6.12",
    "logrocket-vuex": "^0.0.3",
    "register-service-worker": "^1.0.0",
    "rxjs": "^6.2.0",
    "stickyfilljs": "^2.0.5",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuetify": "^1.0.18",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@types/algoliasearch": "^3.27.4",
    "@types/base64-js": "^1.2.5",
    "@types/jest": "^22.2.3",
    "@vue/cli": "^3.0.0-beta.15",
    "@vue/cli-plugin-babel": "^3.0.0-beta.11",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-beta.11",
    "@vue/cli-plugin-pwa": "^3.0.0-beta.11",
    "@vue/cli-plugin-typescript": "^3.0.0-beta.11",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.11",
    "@vue/cli-service": "^3.0.0-beta.11",
    "babel-core": "^6.26.3",
    "eslint": "^4.19.1",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "node-sass": "^7.0.0",
    "precise-commits": "^1.0.2",
    "prettier": "^1.13.4",
    "sass-loader": "^7.0.1",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.10.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "worker-loader": "^2.0.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "<rootDir>/(tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx))"
    ]
  }
}
